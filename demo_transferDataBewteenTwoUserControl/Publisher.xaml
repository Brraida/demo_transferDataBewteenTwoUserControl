<UserControl x:Class="demo_transferDataBewteenTwoUserControl.Publisher"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:demo_transferDataBewteenTwoUserControl"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center" ButtonBase.Click="StackPanel_Click">
            <Button Name="btn1" Content="btn1" Width="40" Height="20" Margin="4"/>
            <Button Name="btn2" Content="btn2" Width="40" Height="20" Margin="4"/>
            <Button Name="btn3" Content="btn3" Width="40" Height="20" Margin="4"/>
            <Button Name="btn4" Content="btn4" Width="40" Height="20" Margin="4"/>
        </StackPanel>
    </Grid>
</UserControl>
